{
  "sitemaps": [
    "https://dgnv.ir/maps/index.xml"
  ],
  "repeatAfter": 7200,
  "crawlerOptions": {
    "concurrency": 3,
    "request_options": {
      "delay": 5000,
      "timeout": 13
    },
    "request_method": "GET",
    "request_headers": {
        "User-Agent": "GitHub-Cache-Warmup/v1"
    }
  },
  "parserOptions": {
    "request_headers": {
      "User-Agent": "GitHub-Cache-Warmup/v1"
    }
  },
  "clientOptions": {
    "verify": false,
    "headers": {"Accept-Encoding": "gzip, deflate, br"}
  },
  "strategy": "sort-by-lastmod",
  "allowFailures": true,
  "stopOnFailure": false,
  "limit": 3,
  "progress": true,
  "format": "text",
  "logFile": "cache-warmup.log",
  "logLevel": "info"
}
