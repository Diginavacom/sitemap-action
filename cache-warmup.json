{
  "$schema": "https://cache-warmup.dev/config.schema.json",
  "sitemaps": [
    "https://dgnv.ir/maps/index.xml"
  ],
  "repeatAfter": 7200,
  "crawlerOptions": {
    "concurrency": 3,
    "requestOptions": {
      "request_method": "GET",
      "delay": 5000,
      "requestHeaders": {
        "User-Agent": "GitHub-Cache-Warmup/1.0.0"
      },
      "timeout": 13
    }
  },
  "strategy": "sort-by-lastmod",
  "allowFailures": true,
  "limit": 700
}
